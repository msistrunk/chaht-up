{
	"parser": "babel-eslint",
	"extends": "airbnb",
	"env": {
		"browser": true
	},
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [
					".ts",
					".tsx",
					".js",
					".jsx"
				]
			}
		}
	},
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx",
					".tsx",
					".ts"
				]
			}
		],
			"sort-imports": ["error", {
			"ignoreCase": false,
			"ignoreMemberSort": false,
			"memberSyntaxSortOrder": ["none", "all", "multiple", "single"]
    }]
	}
}
